.date-number {
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 14px;
  margin-left: 40rpx;
  height:100%;
}

.date-number text {
  line-height: 50%;
}

.highlight {
  font-size: 20px;
}

.line {
  width: 1px;
  height: 40rpx;
  background-color: black;
  margin-left: 10rpx;
  margin-right: 10rpx;
}

.date {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.date text {
  font-size: 12px;
}